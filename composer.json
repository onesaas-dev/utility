{
  "name": "onesaas/utility",
  "description": "A Configuration Package for PHP that uses parsed YAML.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Schmautz",
      "email": "ceo@elixant.ca"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "path",
      "url": "../*",
      "options": {
        "symlink": false
      }
    }
  ],
  "require": {
    "php": ">=7.2",
    "ramsey/uuid": "~3.8",
    "doctrine/inflector": "~1.3",
    "symfony/var-dumper": "~4.3"
  },
  "require-dev": {
    "illuminate/support": "~5.8",
    "illuminate/config": "5.8.*"
  },
  "autoload": {
    "psr-4": {
      "OneSaas\\Components\\Utility\\": "src/"
    },
    "files": [
      "src/Common.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "optimize-autoloader": true,
    "preferred-install": "dist"
  }
}